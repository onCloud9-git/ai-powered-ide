# AI-Powered IDE Environment Configuration

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Other AI Providers
GOOGLE_AI_API_KEY=your_google_ai_key_here
COHERE_API_KEY=your_cohere_api_key_here

# AI Engine Configuration
AI_ENGINE_TIMEOUT=30000
AI_ENGINE_MAX_TOKENS=4000
AI_ENGINE_TEMPERATURE=0.7

# Development Settings
NODE_ENV=development
LOG_LEVEL=info
DEBUG_AI_REQUESTS=false

# MCP Configuration
MCP_PUPPETEER_HEADLESS=true
MCP_PUPPETEER_TIMEOUT=10000

# Terminal Integration
TERMINAL_AI_ENABLED=true
TERMINAL_COMMAND_SUGGESTIONS=true

# Performance Settings
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
MAX_CONCURRENT_AI_REQUESTS=5